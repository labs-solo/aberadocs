{
    "title": "AquaBera Docs",
    "author": "Your Name",
    "description": "Official documentation for AquaBera.",
    "language": "en",
    "plugins": [
      "search",
      "highlight",
      "lunr",
      "theme-default",
      "fontsettings",
      "sharing",
      "tabs",
      "livereload",
      "hints",
      "back-to-top-button",
      "anchor-navigation-ex",
      "copy-code-button",
      "splitter"
    ],
    "pluginsConfig": {
      "fontsettings": {
        "theme": "white",
        "family": "sans",
        "size": 2
      },
      "sharing": {
        "facebook": true,
        "twitter": true,
        "google": false,
        "weibo": false,
        "instapaper": false,
        "vk": false,
        "all": [
          "facebook",
          "twitter"
        ]
      },
      "anchor-navigation-ex": {
        "showLevel": false,
        "associatedWithSummary": true,
        "printLog": false,
        "multipleH1": false,
        "mode": "float",
        "float": {
          "showLevelIcon": false,
          "level1Icon": "fa fa-hand-o-right",
          "level2Icon": "fa fa-hand-o-right",
          "level3Icon": "fa fa-hand-o-right"
        }
      }
    },
    "styles": {
      "website": "styles/custom.css"
    },
    "structure": {
      "readme": "README.md",
      "summary": "SUMMARY.md"
    }
  }
  